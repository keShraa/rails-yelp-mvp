<h1>Add a review</h1>

<div class="jumbotron">
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.button :submit, class:'btn btn-primary' %>
  <% end %>
  <div class="d-flex justify-content-end">
    <%= link_to 'Back', restaurants_path %>
  </div>
</div
